<div class="jumbotron">
    <img src="./public/assets/img/burger-face.jpg">
</div>

<div>
    <h1>BURGERS:</h1>
    <ul>
        {{#each burgers}}
        {{#unless devoured}}
        <li>{{this.burger_name}}</li>
        {{/unless}}
        {{/each}}
    </ul>
</div>

<div>
    <h1>DEVOURED BURGERS:</h1>
    <ul>
        {{#each burgers}}
        {{#if this.devoured}}
        <li>{{this.burger_name}}</li>
        {{/if}}
        {{/each}}
    </ul>
</div>